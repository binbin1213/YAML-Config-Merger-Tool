{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "app": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "./",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": {
              "base": "./dist",
              "browser": "browser"
            },
            "browser": "index.tsx",
            "tsConfig": "tsconfig.json",
            "styles": [
              "src/tailwind-output.css",
              "node_modules/prism-themes/themes/prism-dracula.css"
            ],
            "scripts": [
              "node_modules/js-yaml/dist/js-yaml.min.js"
            ],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "public/",
                "output": "/"
              },
              {
                "glob": "favicon.svg",
                "input": "public/",
                "output": "/"
              }
            ],
            "optimization": true,
            "sourceMap": false,
            "namedChunks": false,
            "extractLicenses": true,
            "baseHref": "/"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "outputHashing": "none"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "options": {
            "port": 3001,
            "buildTarget": "app:build:development"
          },
          "configurations": {
            "production": {
              "buildTarget": "app:build:production"
            },
            "development": {
              "buildTarget": "app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              ".//**/*.ts",
              ".//**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "schematicCollections": [
      "angular-eslint"
    ]
  }
}
